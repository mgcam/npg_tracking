use strict;
use warnings;
use FindBin qw($Bin);
use lib ( -d "$Bin/../lib/perl5" ? "$Bin/../lib/perl5" : "$Bin/../lib" );
use npg::sensors;

our $VERSION = '0';

npg::sensors->new_with_options()->main();

0;

